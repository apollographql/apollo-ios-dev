      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Property accessors are always generated for each field directly included in a selection",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "set in the GraphQL definition. In addition, the code generation engine can compute which",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "selections from a selection set’s parents, sibling inline fragments, and named fragment",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "spreads will also be included on the response object, given the selection set’s scope.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "By default, all possible fields and named fragment accessors are merged into each selection",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "set.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Disabling field merging and ",
                  "type" : "text"
                },
                {
                  "code" : "selectionSetInitializers",
                  "type" : "codeVoice"
                },
                {
                  "text" : " functionality are",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "incompatible. If using ",
                  "type" : "text"
                },
                {
                  "code" : "selectionSetInitializers",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ",
                  "type" : "text"
                },
                {
                  "code" : "fieldMerging",
                  "type" : "codeVoice"
                },
                {
                  "text" : " must be set to ",
                  "type" : "text"
                },
                {
                  "code" : ".all",
                  "type" : "codeVoice"
                },
                {
                  "text" : ",",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "otherwise a validation error will be thrown when runnning code generation.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",

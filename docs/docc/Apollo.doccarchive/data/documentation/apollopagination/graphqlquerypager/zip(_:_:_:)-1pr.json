              "code" : "PassthroughSubject",
              "type" : "codeVoice"
            },
            {
              "text" : " that emit values; ",
              "type" : "text"
            },
            {
              "code" : "Publisher\/zip(_:_:_:)-9yqi1",
              "type" : "codeVoice"
            },
            {
              "text" : " receives the oldest value from each publisher and uses the ",
              "type" : "text"
            },
            {
              "code" : "Int",
              "type" : "codeVoice"
            },
            {
              "text" : " from ",
              "type" : "text"
            },
            {
              "code" : "numbersPub",
              "type" : "codeVoice"
            },
            {
              "text" : " and publishes a string that repeats the ",
              "type" : "text"
            },
            {
              "text" : "doc:\/\/com.apple.documentation\/documentation\/Swift\/String",
              "type" : "text"
            },
            {
              "text" : " from ",
              "type" : "text"

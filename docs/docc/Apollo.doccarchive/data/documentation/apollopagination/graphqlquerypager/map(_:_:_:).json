        },
        {
          "inlineContent" : [
            {
              "text" : "In the following example, the ",
              "type" : "text"
            },
            {
              "code" : "Publisher\/map(_:_:_:)",
              "type" : "codeVoice"
            },
            {
              "text" : " operator uses the Swift key path syntax to access the ",
              "type" : "text"
            },
            {
              "code" : "die1",
              "type" : "codeVoice"
            },
            {
              "text" : ", ",
              "type" : "text"
            },
            {
              "code" : "die2",
              "type" : "codeVoice"

            {
              "inlineContent" : [
                {
                  "text" : "Specifies the maximum number of concurrent publisher subscriptions, or ",
                  "type" : "text"
                },
                {
                  "code" : "Combine\/Subscribers\/Demand\/unlimited",
                  "type" : "codeVoice"
                },
                {
                  "text" : " if unspecified.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
